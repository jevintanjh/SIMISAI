{
    "EndpointConfigName": "simisai-sealion-performance-optimized",
    "ProductionVariants": [
        {
            "VariantName": "performance-optimized",
            "ModelName": "simisai-sealion-gguf-model-v5",
            "InstanceType": "ml.m5.2xlarge",
            "InitialInstanceCount": 1,
            "VariantWeight": 100,
            "ModelDataDownloadTimeoutInSeconds": 3600,
            "ContainerStartupHealthCheckTimeoutInSeconds": 300
        }
    ],
    "DataCaptureConfig": {
        "EnableCapture": true,
        "InitialSamplingPercentage": 50,
        "DestinationS3Uri": "s3://simisai-production-frontend/data-capture/",
        "CaptureOptions": [
            {
                "CaptureMode": "Input"
            },
            {
                "CaptureMode": "Output"
            }
        ],
        "CaptureContentTypeHeader": {
            "CsvContentTypes": ["text/csv"],
            "JsonContentTypes": ["application/json"]
        }
    },
    "Tags": [
        {
            "Key": "Environment",
            "Value": "Production"
        },
        {
            "Key": "Optimization",
            "Value": "Performance"
        },
        {
            "Key": "Purpose",
            "Value": "SIMISAI-LLM"
        }
    ]
}
