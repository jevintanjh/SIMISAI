# AWS SageMaker SealIon Configuration
# Copy this to .env and configure your values

# SageMaker SealIon Endpoint (set after deployment)
SEALION_SAGEMAKER_ENDPOINT=sea-lion-v4-endpoint
SEALION_SAGEMAKER_REGION=us-east-1

# AWS Credentials (if not using IAM roles)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# Database Configuration (required)
DATABASE_URL=your_postgresql_connection_string

# Computer Vision Configuration (optional)
CV_REMOTE_URL=https://dockercv.onrender.com

# Fallback SealIon API Configuration (optional, for external API fallback)
# SEALION_API_KEY=your_external_api_key
# SEALION_API_URL=https://api.sealion.example.com/v1

# Development Settings
NODE_ENV=development
PORT=3001